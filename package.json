{
  "name": "@wrygiel/log4js-loggly-appender",
  "version": "1.0.0",
  "description": "Loggly Appender for log4js-node",
  "homepage": "https://github.com/wrygiel/log4js-loggly-appender",
  "files": [
    "lib",
    "types"
  ],
  "keywords": [
    "logging",
    "log",
    "log4j",
    "node",
    "loggly"
  ],
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "author": "Gareth Jones <gareth.nomiddlename@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/log4js-node/loggly.git"
  },
  "bugs": {
    "url": "http://github.com/log4js-node/loggly/issues"
  },
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "npm": "please-use-pnpm",
    "pnpm": ">= 8.0.0",
    "yarn": "please-use-pnpm"
  },
  "scripts": {
    "update-major": "npx npm-check-updates -u && pnpm install",
    "update-minor": "npx npm-check-updates --target minor -u && pnpm install"
  },
  "directories": {
    "test": "test",
    "lib": "lib"
  },
  "dependencies": {
    "debug": "^3.2.7",
    "node-loggly-bulk": "^2.2.5"
  },
  "devDependencies": {
    "@log4js-node/sandboxed-module": "^2.2.1",
    "log4js": "^6.9.1"
  },
  "browser": {
    "os": false
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "example",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 72,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": "\n# allowed type: feat, fix, docs, style, refactor, example, perf, test, chore, revert\n# subject no more than 50 chars\n# a body line no more than 72 chars"
    }
  }
}
